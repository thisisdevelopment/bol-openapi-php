{
    "name": "thisisdevelopment/bol-openapi-php",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "suggest": {
        "illuminate/support": "Allows for easy integration with the Laravel framework"
    },
    "autoload": {
        "psr-4": {
            "ThisIsDevelopment\\OpenApi\\Bol\\Advertiser\\" : [
                "src/Support/lib-advertiser/",
                "src/Advertiser/lib/"
            ],
            "ThisIsDevelopment\\OpenApi\\Bol\\Retailer\\" : [
                "src/Support/lib-retailer/",
                "src/Retailer/lib/"
            ],
            "ThisIsDevelopment\\OpenApi\\Bol\\Shared\\" : [
                "src/Support/lib-shared/",
                "src/Shared/lib/"
            ],
            "ThisIsDevelopment\\OpenApi\\Bol\\Support\\" : "src/Support/lib/"
        }
    }
}
